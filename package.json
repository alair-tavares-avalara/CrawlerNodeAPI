  "bugs": {
    "url": "https://github.com/alairjt/CrawlerNodeAPI/issues"
  },
  "homepage": "https://github.com/alairjt/CrawlerNodeAPI#readme",
  "dependencies": {
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "mongoose": "^4.12.6",
    "nodemon": "^1.12.1"
  },
  "devDependencies": {
    "codecov": "^1.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^2.0.0",
    "istanbul": "^0.3.2",
    "jsdoc": "^3.4.0",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0"
